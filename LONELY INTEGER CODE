package com.go;
import java.util.Arrays;
public class LonelyInteger {
	public static void main(String[] args) {
		int []a= {1,1,2,3,3,4,4,5,5,6,6};
		Arrays.sort(a);
		for(int i=0;i<=a.length-2;i=i+2) {
			if(a[i]!=a[i+1]) {
				System.out.println(a[i]);
				System.exit(0);;
			}
			
		}
		System.out.println(a[a.length-1]);
		
	}

}
